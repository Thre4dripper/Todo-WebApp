<%- include("header") -%> 
<body>
    <div class="box" id="heading">
        <h1><%= TodoListName %> </h1>
    </div>

    <div class="box">
        <%  for(var i=0;i<listItems.length;i++) { %>
        <div class="item">
            <input type="checkbox">
            <p> <%= listItems[i]  %> </p>
        </div>
    <% } %> 

    <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Todo" autocomplete="off">
        <button type="submit" name="button">+</button>
    </form>
    </div>
</body>
</html>